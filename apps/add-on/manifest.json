{

  "description": "Web addons for communicate to webcomplete neovim plugin",
  "manifest_version": 2,
  "name": "Webcomplete addons",
  "version": "1.0",
  "icons": {
    "48": "icons/message.svg"
  },

  "applications": {
    "gecko": {
      "id": "webcomplete@example.org",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/message.svg"
  },

  "permissions": [
    "<all_urls>",
    "nativeMessaging",
    "activeTab"
  ]

}
